<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="309.0" prefWidth="445.0" spacing="10.0" 
      xmlns="http://javafx.com/javafx/8.0.201" xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="eu.ggnet.calculator.ui.modification.SetCertificationAtSelectedPupilController">
    <HBox alignment="CENTER" prefHeight="30.0" prefWidth="338.0">
        <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Enter integral valus (0-15) for each subject to add a certification:" />
        </children>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </HBox>
    <HBox alignment="CENTER" spacing="25.0">
        <Label text="Business english: " />
        <TextField fx:id="engInput" alignment="CENTER_RIGHT" />
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </HBox>
    <HBox alignment="CENTER" spacing="25.0">
        <Label text=" IT systems: " />
        <TextField fx:id="itsInput" alignment="CENTER_RIGHT" />
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <padding>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </padding>
    </HBox>
    <HBox alignment="CENTER" spacing="25.0">
        <Label text="IT workshop: " />
        <TextField fx:id="itwInput" alignment="CENTER_RIGHT" />
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </HBox>
    <HBox alignment="CENTER" spacing="25.0">
        <Label text="economics and society: " />
        <TextField fx:id="socInput" alignment="CENTER_RIGHT" />
        <VBox.margin>
            <Insets />
        </VBox.margin>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </HBox>
    <HBox alignment="CENTER" spacing="25.0">
        <Label text="software development: " />
        <TextField fx:id="devInput" alignment="CENTER_RIGHT" />
        <VBox.margin>
            <Insets />
        </VBox.margin>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </HBox>
    <HBox alignment="CENTER" spacing="25.0">
        <Label text="business processes:" />
        <TextField fx:id="orgaInput" alignment="CENTER_RIGHT" />
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
        <VBox.margin>
            <Insets />
        </VBox.margin>
    </HBox>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="25.0">
        <children>
            <Button fx:id="createButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#create" text="Grade" />
            <Button fx:id="cancelButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
        </children>
        <VBox.margin>
            <Insets />
        </VBox.margin>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </HBox>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
